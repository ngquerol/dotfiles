# GREP
alias grep='grep --color=auto'

# LESS
alias less='less -m'

# EDITOR
alias e=$EDITOR

# MISC
alias reload='source ~/.zshrc'
alias bup='brew update && brew upgrade --all && brew-cask-upgrade.sh'
